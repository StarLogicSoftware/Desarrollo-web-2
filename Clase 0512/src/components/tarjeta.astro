---
const { titulo, contenido, href, link } = Astro.props
---

<article>
	<div class="card" style="width: 18rem;">
		<img src="..." class="card-img-top" alt="..." />
		<div class="card-body">
			<h5 class="card-title">{titulo}</h5>
			<p class="card-text">{contenido}</p>
			<a href={href} class="btn btn-primary">{link}</a>
		</div>
	</div>
</article>

